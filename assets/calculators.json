[
  {"name":"BMI (Body Mass Index)","name_ru":"ИМТ (BMI)","description":"Body Mass Index","description_ru":"Индекс массы тела","category":"Anthropometry","category_ru":"Антропометрия","formula":"{weight} / (({height}/100)^2)","input_fields":[{"name":"weight","label":"Weight (kg)","type":"number"},{"name":"height","label":"Height (cm)","type":"number"}],"interpretation_rules":[]},
  {"name":"BSA (Mosteller)","name_ru":"Площадь поверхности тела (Mosteller)","description":"Body Surface Area (Mosteller)","description_ru":"Площадь поверхности тела (Mosteller)","category":"Anthropometry","category_ru":"Антропометрия","formula":"sqrt(({height} * {weight})/3600)","input_fields":[{"name":"weight","label":"Weight (kg)","type":"number"},{"name":"height","label":"Height (cm)","type":"number"}] ,"interpretation_rules":[]},
  {"name":"LBM (Janmahasatian)","name_ru":"Безжировая масса (Janmahasatian)","description":"Lean Body Mass (Janmahasatian)","description_ru":"Безжировая масса тела (Janmahasatian)","category":"Anthropometry","category_ru":"Антропометрия","formula":"( {sex} == 'female' ? (9270 * {weight}) / (8780 + 244 * ({weight} / (({height}/100)^2))) : (9270 * {weight}) / (6680 + 216 * ({weight} / (({height}/100)^2))) )","input_fields":[{"name":"weight","label":"Weight (kg)","type":"number"},{"name":"height","label":"Height (cm)","type":"number"},{"name":"sex","label":"Sex","type":"select","options":["male","female"]}],"interpretation_rules":[]},
  {"name":"Ideal Body Weight (IBW)","name_ru":"Идеальная масса тела (IBW)","description":"Ideal Body Weight","description_ru":"Идеальная масса тела","category":"Anthropometry","category_ru":"Антропометрия","formula":"( {sex} == 'female' ? 45.5 + 0.9 * ({height} - 152) : 50 + 0.9 * ({height} - 152) )","input_fields":[{"name":"height","label":"Height (cm)","type":"number"},{"name":"sex","label":"Sex","type":"select","options":["male","female"]}],"interpretation_rules":[]},
  {"name":"Adjusted Body Weight (AdjBW)","name_ru":"Скорректированная масса тела (AdjBW)","description":"Adjusted Body Weight for dosing","description_ru":"Скорректированная масса тела","category":"Anthropometry","category_ru":"Антропометрия","formula":"{ibw} + 0.4 * ({weight} - {ibw})","input_fields":[{"name":"weight","label":"Weight (kg)","type":"number"},{"name":"ibw","label":"IBW (kg)","type":"number"}],"interpretation_rules":[]},

  {"name":"FRIED score","name_ru":"Формула Фрид (FRIED)","description":"Frailty assessment (FRIED)","description_ru":"Расчет риска (FRIED)","category":"Cardiology","category_ru":"Кардиология","formula":"(placeholder)","input_fields":[],"interpretation_rules":[]},
  {"name":"CHA2DS2-VASc","name_ru":"CHA₂DS₂-VASc","description":"Stroke risk in atrial fibrillation","description_ru":"Риск инсульта при фибрилляции предсердий","category":"Cardiology","category_ru":"Кардиология","formula":"( ({age} >= 75) ? 2 : ( ({age} >= 65) ? 1 : 0 ) ) + ({chf} ? 1 : 0) + ({hypertension} ? 1 : 0) + ({stroke} ? 2 : 0) + ({vascular} ? 1 : 0) + ({diabetes} ? 1 : 0) + ({sex} == 'female' ? 1 : 0)","input_fields":[{"name":"age","label":"Age","type":"number"},{"name":"sex","label":"Sex","type":"select","options":["male","female"]},{"name":"chf","label":"Heart failure (1=yes,0=no)","type":"number","min":0,"max":1},{"name":"hypertension","label":"Hypertension (1/0)","type":"number","min":0,"max":1},{"name":"stroke","label":"Prior stroke/TIA (1/0)","type":"number","min":0,"max":1},{"name":"vascular","label":"Vascular disease (1/0)","type":"number","min":0,"max":1},{"name":"diabetes","label":"Diabetes (1/0)","type":"number","min":0,"max":1}],"interpretation_rules":[]},
  {"name":"HAS-BLED","name_ru":"HAS-BLED","description":"Bleeding risk on anticoagulation","description_ru":"Риск кровотечений на антикоагулянтах","category":"Cardiology","category_ru":"Кардиология","formula":"({hypertension} ? 1 : 0) + ({abnormalRenal} ? 1 : 0) + ({abnormalLiver} ? 1 : 0) + ({stroke} ? 1 : 0) + ({bleedingHistory} ? 1 : 0) + ({labileINR} ? 1 : 0) + ({age} > 65 ? 1 : 0) + ({drugs} ? 1 : 0) + ({alcohol} ? 1 : 0)","input_fields":[{"name":"hypertension","label":"Hypertension (1/0)","type":"number","min":0,"max":1},{"name":"abnormalRenal","label":"Abnormal renal function (1/0)","type":"number","min":0,"max":1},{"name":"abnormalLiver","label":"Abnormal liver function (1/0)","type":"number","min":0,"max":1},{"name":"stroke","label":"Prior stroke (1/0)","type":"number","min":0,"max":1},{"name":"bleedingHistory","label":"Bleeding history (1/0)","type":"number","min":0,"max":1},{"name":"labileINR","label":"Labile INR (1/0)","type":"number","min":0,"max":1},{"name":"age","label":"Age","type":"number"},{"name":"drugs","label":"Drugs predisposing to bleeding (1/0)","type":"number","min":0,"max":1},{"name":"alcohol","label":"Alcohol (1/0)","type":"number","min":0,"max":1}],"interpretation_rules":[]},
  {"name":"QRISK / ASCVD","name_ru":"QRISK / ASCVD","description":"Cardiovascular risk calculators","description_ru":"Риск сердечно-сосудистых событий","category":"Cardiology","category_ru":"Кардиология","formula":"(placeholder)","input_fields":[],"interpretation_rules":[]},
  {"name":"GRACE score","name_ru":"GRACE score","description":"Prognosis in ACS","description_ru":"Прогноз при ОКС","category":"Cardiology","category_ru":"Кардиология","formula":"(placeholder)","input_fields":[],"interpretation_rules":[]},
  {"name":"TIMI score","name_ru":"TIMI score","description":"Risk in acute coronary syndromes","description_ru":"Оценка риска при ОКС","category":"Cardiology","category_ru":"Кардиология","formula":"(placeholder)","input_fields":[],"interpretation_rules":[]},
  {"name":"Killip class","name_ru":"Killip class","description":"Heart failure severity class","description_ru":"Степень сердечной недостаточности","category":"Cardiology","category_ru":"Кардиология","formula":"(placeholder)","input_fields":[],"interpretation_rules":[]},

  {"name":"eGFR Cockcroft-Gault","name_ru":"eGFR Cockcroft-Gault","description":"Estimate GFR (Cockcroft-Gault)","description_ru":"eGFR Cockcroft-Gault","category":"Renal","category_ru":"Почечная функция","formula":"( ((140 - {age}) * {weight}) / (72 * {creatinine}) ) * ( {sex} == 'female' ? 0.85 : 1 )","input_fields":[{"name":"age","label":"Age","type":"number"},{"name":"weight","label":"Weight (kg)","type":"number"},{"name":"creatinine","label":"Serum creatinine (mg/dL)","type":"number"},{"name":"sex","label":"Sex","type":"select","options":["male","female"]}],"interpretation_rules":[]},
  {"name":"eGFR MDRD","name_ru":"eGFR MDRD","description":"Estimate GFR using simplified MDRD","description_ru":"eGFR MDRD","category":"Renal","category_ru":"Почечная функция","formula":"175 * ({creatinine} ^ -1.154) * ({age} ^ -0.203) * ({sex} == 'female' ? 0.742 : 1)","input_fields":[{"name":"creatinine","label":"Serum creatinine (mg/dL)","type":"number"},{"name":"age","label":"Age","type":"number"},{"name":"sex","label":"Sex","type":"select","options":["male","female"]}],"interpretation_rules":[]},
  {"name":"24h Creatinine Clearance","name_ru":"Клиренс креатинина (24ч)","description":"24-hour urine creatinine clearance","description_ru":"Клиренс креатинина по суточной моче","category":"Renal","category_ru":"Почечная функция","formula":"({uCr} * {urineVolume}) / ({pCr} * 1440)","input_fields":[{"name":"uCr","label":"Urine creatinine (mg/dL)","type":"number"},{"name":"urineVolume","label":"Urine volume (mL/24h)","type":"number"},{"name":"pCr","label":"Plasma creatinine (mg/dL)","type":"number"}],"interpretation_rules":[]},
  {"name":"FENa","name_ru":"Фракционная экскреция натрия (FENa)","description":"Fractional excretion of sodium","description_ru":"Фракционная экскреция натрия","category":"Renal","category_ru":"Почечная функция","formula":"(({uNa} * {pCr}) / ({pNa} * {uCr})) * 100","input_fields":[{"name":"uNa","label":"Urine sodium (mmol/L)","type":"number"},{"name":"pNa","label":"Plasma sodium (mmol/L)","type":"number"},{"name":"uCr","label":"Urine creatinine (mg/dL)","type":"number"},{"name":"pCr","label":"Plasma creatinine (mg/dL)","type":"number"}],"interpretation_rules":[]},

  {"name":"A-a gradient","name_ru":"Альвеолярно-артериальный градиент (A–a)","description":"Alveolar-arterial gradient (approx)","description_ru":"А–a градиент (прибл.)","category":"Respiratory","category_ru":"Лёгкие и дыхание","formula":"( ({fio2} * (760 - 47)) - ({pCO2} / 0.8) ) - {paO2}","input_fields":[{"name":"paO2","label":"PaO2 (mmHg)","type":"number"},{"name":"pCO2","label":"PaCO2 (mmHg)","type":"number"},{"name":"fio2","label":"FiO2 (fraction)","type":"number"}],"interpretation_rules":[]},
  {"name":"PaO2/FiO2 ratio","name_ru":"PaO₂/FiO₂","description":"Oxygenation index","description_ru":"PaO₂/FiO₂ отношение","category":"Respiratory","category_ru":"Лёгкие и дыхание","formula":"{paO2} / {fio2}","input_fields":[{"name":"paO2","label":"PaO2 (mmHg)","type":"number"},{"name":"fio2","label":"FiO2 (fraction)","type":"number"}],"interpretation_rules":[]},
  {"name":"Spirometry quick ratio (FEV1/FVC)","name_ru":"FEV1/FVC","description":"FEV1/FVC ratio (input-based)","description_ru":"FEV1/FVC отношение","category":"Respiratory","category_ru":"Лёгкие и дыхание","formula":"{fev1} / {fvc}","input_fields":[{"name":"fev1","label":"FEV1 (L)","type":"number"},{"name":"fvc","label":"FVC (L)","type":"number"}],"interpretation_rules":[]},

  {"name":"ESR expected (Westergren approx)","name_ru":"Ожидаемая СОЭ","description":"Approx expected ESR by age/sex","description_ru":"Ожидаемая СОЭ по возрасту/полу","category":"Hematology","category_ru":"Гематология","formula":"({sex} == 'female' ? (({age} + 10) / 2) : ({age} / 2))","input_fields":[{"name":"age","label":"Age","type":"number"},{"name":"sex","label":"Sex","type":"select","options":["male","female"]}],"interpretation_rules":[]},
  {"name":"MCV and MCHC","name_ru":"MCV и MCHC","description":"Mean corpuscular volume and MCHC","description_ru":"MCV и MCHC расчёт","category":"Hematology","category_ru":"Гематология","formula":"{hct} > 0 && {rbc} > 0 ? ({hct} * 10 / {rbc}) : null","input_fields":[{"name":"hct","label":"Hematocrit (%)","type":"number"},{"name":"rbc","label":"RBC (10^6/µL)","type":"number"},{"name":"hb","label":"Hemoglobin (g/dL)","type":"number"}],"interpretation_rules":[]},

  {"name":"HOMA-IR","name_ru":"Гомеостазный индекс (HOMA-IR)","description":"HOMA-IR insulin resistance index","description_ru":"Гомеостазный индекс инсулинорезистентности","category":"Endocrinology","category_ru":"Эндокринология","formula":"({glucose} * {insulin}) / 405","input_fields":[{"name":"glucose","label":"Fasting glucose (mg/dL)","type":"number"},{"name":"insulin","label":"Fasting insulin (µU/mL)","type":"number"}],"interpretation_rules":[]},
  {"name":"BMR (Mifflin-St Jeor)","name_ru":"Базальный метаболизм (Mifflin-St Jeor)","description":"Basal metabolic rate (Mifflin-St Jeor)","description_ru":"Базальный метаболизм (Mifflin-St Jeor)","category":"Endocrinology","category_ru":"Эндокринология","formula":"( {sex} == 'female' ? (10 * {weight} + 6.25 * {height} - 5 * {age} - 161) : (10 * {weight} + 6.25 * {height} - 5 * {age} + 5) )","input_fields":[{"name":"weight","label":"Weight (kg)","type":"number"},{"name":"height","label":"Height (cm)","type":"number"},{"name":"age","label":"Age","type":"number"},{"name":"sex","label":"Sex","type":"select","options":["male","female"]}],"interpretation_rules":[]},
  {"name":"BAI (Body Adiposity Index)","name_ru":"Индекс жировой массы (BAI)","description":"Body Adiposity Index","description_ru":"Индекс жировой массы","category":"Endocrinology","category_ru":"Эндокринология","formula":"({hip} / (({height}/100) ^ 1.5)) - 18","input_fields":[{"name":"hip","label":"Hip circumference (cm)","type":"number"},{"name":"height","label":"Height (cm)","type":"number"}],"interpretation_rules":[]},

  {"name":"SIRS criteria","name_ru":"Критерии SIRS","description":"Systemic inflammatory response syndrome criteria","description_ru":"Критерии SIRS","category":"Infection","category_ru":"Инфекции и воспаление","formula":"( ({temp} > 38 || {temp} < 36) ? 1 : 0 ) + ({hr} > 90 ? 1 : 0) + ({rr} > 20 ? 1 : 0) + ({wbc} > 12 || {wbc} < 4 ? 1 : 0)","input_fields":[{"name":"temp","label":"Temperature (°C)","type":"number"},{"name":"hr","label":"Heart rate (bpm)","type":"number"},{"name":"rr","label":"Respiratory rate (breaths/min)","type":"number"},{"name":"wbc","label":"WBC (10^9/L)","type":"number"}],"interpretation_rules":[]},
  {"name":"qSOFA","name_ru":"qSOFA","description":"Quick SOFA score","description_ru":"qSOFA","category":"Infection","category_ru":"Инфекции и воспаление","formula":"({rr} >= 22 ? 1 : 0) + ({sbp} <= 100 ? 1 : 0) + ({gcs} < 15 ? 1 : 0)","input_fields":[{"name":"rr","label":"Respiratory rate","type":"number"},{"name":"sbp","label":"Systolic BP (mmHg)","type":"number"},{"name":"gcs","label":"GCS total","type":"number"}],"interpretation_rules":[]},
  {"name":"CURB-65","name_ru":"CURB-65","description":"Pneumonia severity score","description_ru":"CURB-65","category":"Infection","category_ru":"Инфекции и воспаление","formula":"({confusion} ? 1 : 0) + ({bun} > 7 ? 1 : 0) + ({rr} >= 30 ? 1 : 0) + ({sbp} < 90 || {dbp} <= 60 ? 1 : 0) + ({age} >= 65 ? 1 : 0)","input_fields":[{"name":"confusion","label":"Confusion (1/0)","type":"number","min":0,"max":1},{"name":"bun","label":"BUN (mmol/L)","type":"number"},{"name":"rr","label":"Respiratory rate","type":"number"},{"name":"sbp","label":"Systolic BP","type":"number"},{"name":"dbp","label":"Diastolic BP","type":"number"},{"name":"age","label":"Age","type":"number"}],"interpretation_rules":[]},
  {"name":"APACHE II (manual entry)","name_ru":"APACHE II","description":"APACHE II score (enter total)","description_ru":"APACHE II (ввод суммы)","category":"ICU","category_ru":"Интенсивная терапия","formula":"{score}","input_fields":[{"name":"score","label":"APACHE II total score","type":"number"}],"interpretation_rules":[]},
  {"name":"MEWS (simple)","name_ru":"MEWS","description":"Modified Early Warning Score (simple)","description_ru":"MEWS (упрощённый)","category":"Infection","category_ru":"Инфекции и воспаление","formula":"( ({sbp} <= 70) ? 3 : (({sbp} <= 80) ? 2 : (({sbp} <= 100) ? 1 : 0)) ) + ( ({hr} >= 130) ? 3 : (({hr} >= 111) ? 2 : (({hr} >= 101) ? 1 : 0)) ) + ( ({rr} >= 30) ? 3 : (({rr} >= 21) ? 2 : (({rr} >= 9) ? 1 : 0)) ) + ( ({temp} < 35 || {temp} > 38.5) ? 2 : 0 )","input_fields":[{"name":"sbp","label":"Systolic BP","type":"number"},{"name":"hr","label":"Heart rate","type":"number"},{"name":"rr","label":"Respiratory rate","type":"number"},{"name":"temp","label":"Temperature (°C)","type":"number"}],"interpretation_rules":[]},

  {"name":"Revised Trauma Score (RTS) (manual)","name_ru":"RTS","description":"Revised Trauma Score (enter total)","description_ru":"RTS (ввод суммы)","category":"Surgery","category_ru":"Хирургия и травма","formula":"{score}","input_fields":[{"name":"score","label":"RTS total","type":"number"}],"interpretation_rules":[]},
  {"name":"Glasgow Coma Scale (GCS)","name_ru":"GCS","description":"Level of consciousness","description_ru":"Шкала комы Глазго","category":"Surgery","category_ru":"Хирургия и травма","formula":"{eye_opening} + {verbal_response} + {motor_response}","input_fields":[{"name":"eye_opening","label":"Eye opening (1-4)","type":"number","min":1,"max":4},{"name":"verbal_response","label":"Verbal response (1-5)","type":"number","min":1,"max":5},{"name":"motor_response","label":"Motor response (1-6)","type":"number","min":1,"max":6}],"interpretation_rules":[]},
  {"name":"Abdominal Trauma Index (manual)","name_ru":"Abdominal Trauma Index","description":"Abdominal trauma severity (enter)","description_ru":"Индекс травмы живота (ввод)","category":"Surgery","category_ru":"Хирургия и травма","formula":"{score}","input_fields":[{"name":"score","label":"Index score","type":"number"}],"interpretation_rules":[]},
  {"name":"Caprini score (manual)","name_ru":"Caprini score","description":"VTE risk assessment (enter total)","description_ru":"Риск тромбоэмболии (Caprini) (ввод)","category":"Surgery","category_ru":"Хирургия и травма","formula":"{score}","input_fields":[{"name":"score","label":"Caprini total","type":"number"}],"interpretation_rules":[]},

  {"name":"Child-Pugh (manual)","name_ru":"Child-Pugh","description":"Child-Pugh score (enter total)","description_ru":"Оценка функций печени (Child-Pugh) (ввод)","category":"Hepatology","category_ru":"Гепатология","formula":"{score}","input_fields":[{"name":"score","label":"Child-Pugh total","type":"number"}],"interpretation_rules":[]},
  {"name":"MELD","name_ru":"MELD","description":"Model for end-stage liver disease (MELD)","description_ru":"MELD","category":"Hepatology","category_ru":"Гепатология","formula":"(3.78 * log({bilirubin})) + (11.2 * log({inr})) + (9.57 * log({creatinine})) + 6.43","input_fields":[{"name":"bilirubin","label":"Bilirubin (mg/dL)","type":"number"},{"name":"inr","label":"INR","type":"number"},{"name":"creatinine","label":"Serum creatinine (mg/dL)","type":"number"}],"interpretation_rules":[]},

  {"name":"NIH Stroke Scale (manual)","name_ru":"NIHSS","description":"NIHSS (enter total)","description_ru":"NIHSS (ввод)","category":"Neurology","category_ru":"Нейро и психо оценки","formula":"{score}","input_fields":[{"name":"score","label":"NIHSS total","type":"number"}],"interpretation_rules":[]},
  {"name":"MMSE / MoCA (manual)","name_ru":"MMSE / MoCA","description":"Cognitive screening tests (enter score)","description_ru":"Когнитивные тесты (ввод)","category":"Neurology","category_ru":"Нейро и психо оценки","formula":"{score}","input_fields":[{"name":"score","label":"Test score","type":"number"}],"interpretation_rules":[]},

  {"name":"Naegele's Rule","name_ru":"Правило Наегеле","description":"Estimated due date calculation","description_ru":"Срок беременности (Наегеле)","category":"OB/GYN","category_ru":"OB/GYN","formula":"{lmp} + 280 days","input_fields":[],"interpretation_rules":[]},
  {"name":"Bishop score (manual)","name_ru":"Bishop score","description":"Bishop score (enter total)","description_ru":"Оценка шейки матки (Bishop) (ввод)","category":"OB/GYN","category_ru":"OB/GYN","formula":"{score}","input_fields":[{"name":"score","label":"Bishop total","type":"number"}],"interpretation_rules":[]},

  {"name":"Pediatric Weight Percentiles (manual)","name_ru":"Процентиль веса у детей","description":"Weight percentile (chart required)","description_ru":"Вес по процентилям (ввод)","category":"Pediatrics","category_ru":"Педиатрия","formula":"{percentile}","input_fields":[{"name":"percentile","label":"Percentile (%)","type":"number"}],"interpretation_rules":[]},
  {"name":"APGAR score","name_ru":"APGAR","description":"Newborn assessment","description_ru":"Оценка новорожденного (APGAR)","category":"Pediatrics","category_ru":"Педиатрия","formula":"{appearance} + {pulse} + {grimace} + {activity} + {respiration}","input_fields":[{"name":"appearance","label":"Appearance (0-2)","type":"number","min":0,"max":2},{"name":"pulse","label":"Pulse (0-2)","type":"number","min":0,"max":2},{"name":"grimace","label":"Grimace (0-2)","type":"number","min":0,"max":2},{"name":"activity","label":"Activity (0-2)","type":"number","min":0,"max":2},{"name":"respiration","label":"Respiration (0-2)","type":"number","min":0,"max":2}],"interpretation_rules":[]},
  {"name":"PEWS (manual)","name_ru":"PEWS","description":"Pediatric Early Warning Score (enter total)","description_ru":"PEWS (ввод)","category":"Pediatrics","category_ru":"Педиатрия","formula":"{score}","input_fields":[{"name":"score","label":"PEWS total","type":"number"}],"interpretation_rules":[]},

  {"name":"TNM staging support (manual)","name_ru":"TNM staging","description":"TNM staging helper (enter stage)","description_ru":"TNM стадирование (ввод)","category":"Oncology","category_ru":"Онкология / Радиология","formula":"{stage}","input_fields":[{"name":"stage","label":"Stage (e.g., IIIB)","type":"text"}],"interpretation_rules":[]},
  {"name":"RECIST criteria (manual)","name_ru":"RECIST","description":"RECIST response (enter category)","description_ru":"Критерии RECIST (ввод)","category":"Oncology","category_ru":"Онкология / Радиология","formula":"{category}","input_fields":[{"name":"category","label":"RECIST category","type":"text"}],"interpretation_rules":[]},

  {"name":"Antibiotic dose by weight (simple)","name_ru":"Дозировка антибиотиков по весу","description":"Dose by weight (mg/kg)","description_ru":"Дозировка антибиотиков (мг/кг)","category":"Dosing","category_ru":"Лекарственные дозы","formula":"{dosePerKg} * {weight}","input_fields":[{"name":"dosePerKg","label":"Dose (mg/kg)","type":"number"},{"name":"weight","label":"Weight (kg)","type":"number"}],"interpretation_rules":[]},
  {"name":"Chemo dosing (BSA)","name_ru":"Химиотерапевтические дозы (BSA)","description":"Chemo dosing by BSA","description_ru":"Дозы химиотерапии по BSA","category":"Dosing","category_ru":"Лекарственные дозы","formula":"{dosePerM2} * {bsa}","input_fields":[{"name":"dosePerM2","label":"Dose per m^2","type":"number"},{"name":"bsa","label":"BSA (m^2)","type":"number"}],"interpretation_rules":[]},
  {"name":"Opioid conversion (oral morphine eq)","name_ru":"Конвертация опиоидов","description":"Convert to oral morphine equivalents","description_ru":"Конвертация в эквивалент морфина перорально","category":"Dosing","category_ru":"Лекарственные дозы","formula":"{amount} * {conversionFactor}","input_fields":[{"name":"amount","label":"Dose amount","type":"number"},{"name":"conversionFactor","label":"Conversion factor to oral morphine","type":"number"}],"interpretation_rules":[]},

  {"name":"QTc (Bazett, Fridericia)","name_ru":"QTc расчет","description":"QTc correction formulas","description_ru":"QTc (Bazett, Fridericia)","category":"ECG","category_ru":"ЭКГ и аритмии","formula":"{qt} / sqrt(60 / {hr})","input_fields":[{"name":"qt","label":"QT interval (ms)","type":"number"},{"name":"hr","label":"Heart rate (bpm)","type":"number"}],"interpretation_rules":[]},
  {"name":"Sokolow-Lyon LVH","name_ru":"Sokolow-Lyon","description":"ECG LVH criteria (S in V1 + R in V5)","description_ru":"Критерии LVH (Sokolow-Lyon)","category":"ECG","category_ru":"ЭКГ и аритмии","formula":"{sV1} + {rV5}","input_fields":[{"name":"sV1","label":"S in V1 (mm)","type":"number"},{"name":"rV5","label":"R in V5 (mm)","type":"number"}],"interpretation_rules":[]},

