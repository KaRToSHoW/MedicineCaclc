{
  "name": "expo-rails-7x-template",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "./scripts/start-web-with-proxy.sh",
    "start:expo-only": "npx dotenv-cli -- ./scripts/start-expo.sh",
    "web": "npx dotenv-cli -- ./scripts/start-expo.sh --web",
    "android": "npx dotenv-cli -- ./scripts/start-expo.sh --android",
    "ios": "npx dotenv-cli -- ./scripts/start-expo.sh --ios",
    "start-backend": "python3 api/main.py",
    "gen": "node scripts/generators/index.js",
    "test": "run-p lint test:jest",
    "test:jest": "jest",
    "lint": "run-p lint:eslint lint:types",
    "lint:eslint": "eslint .",
    "lint:types": "tsc --noEmit",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "seed:firebase": "node scripts/seed_calculators_firestore.js",
    "seed:firebase:dry": "node scripts/seed_calculators_firestore.js --dry"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.2.3",
    "@react-navigation/drawer": "^7.7.10",
    "@react-navigation/native": "^7.1.26",
    "ajv": "^8.17.1",
    "expo": "~51.0.0",
    "expo-clipboard": "~6.0.3",
    "expo-constants": "~16.0.2",
    "expo-image-picker": "~15.1.0",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.23",
    "expo-secure-store": "~13.0.2",
    "expo-status-bar": "~1.12.1",
    "firebase": "^10.14.1",
    "firebase-admin": "^13.6.0",
    "http-proxy": "^1.18.1",
    "mathjs": "^12.4.0",
    "nativewind": "^4.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "^2.30.0",
    "react-native-reanimated": "^4.2.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "^15.15.1",
    "react-native-web": "~0.19.10",
    "react-native-worklets": "^0.7.1",
    "react-native-worklets-core": "^1.6.2",
    "tailwindcss": "^3.4.18",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@testing-library/react-native": "~12.4.0",
    "@types/jest": "~29.5.0",
    "@types/react": "~18.2.45",
    "@typescript-eslint/eslint-plugin": "~6.21.0",
    "@typescript-eslint/parser": "~6.21.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "dotenv-cli": "^7.3.0",
    "eslint": "~8.57.0",
    "eslint-config-expo": "~7.1.2",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "expo-cli": "^6.3.12",
    "jest": "~29.7.0",
    "jest-expo": "~51.0.0",
    "npm-run-all": "~4.1.5",
    "react-test-renderer": "^18.2.0",
    "typescript": "~5.3.3"
  },
  "private": true,
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|firebase|@firebase)"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
